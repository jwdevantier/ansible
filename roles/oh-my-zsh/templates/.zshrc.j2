# Load configuration, BSD-init style
####################################
# Distribution defaults

if [[ -d "{{ozsh__cfg_dist}}" ]]; then
  cfgs=`ls "{{ozsh__cfg_dist}}/"*.zsh` 2>/dev/null
  for i in $cfgs; do
    source "$i"
  done
fi

